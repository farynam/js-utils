{
  "name": "js-utils",
  "version": "1.0.0",
  "description": "Utils package",
  "main": "index.js",
  "author": "Marcin Faryna",
  "license": "Apache License 2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/farynam/js-utils.git"
  },
  "scripts": {
