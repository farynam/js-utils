    "@babel/preset-env": "7.4.5",
    "babel-core": "^6.26.3"
  },

  "devDependencies": {
    "jest": "24.9.0"
  },

  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry"
        }
      ]
    ],
    "ignore": [
      "**/excl/*"
    ]
  }
