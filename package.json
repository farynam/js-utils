{
  "name": "mfitbs-js-utils",
  "version": "1.0.12",
  "description": "Utils package",
  "main": "index.js",
  "author": "Marcin Faryna",
  "license": "Apache License 2.0",
  "keywords": [
    "private method",
    "java",
    "private access",
    "access private members",
    "public to private",
    "private methods es6",
    "private methods es6 class",
    "method scoping",
    "field scope",
    "field scoping",
    "utility",
    "helper"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/farynam/mfitbs-js-utils.git"
  },
  "scripts": {
    "build" : "gulp build",
    "pub": "gulp",
    "test": "node_modules/.bin/jest",
    "runapp": "node target/.compiled/example/index.js",
    "compile": "babel src/js --out-dir target/.compiled/app --source-maps --ignore '**/excl/*' --copy-files"
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/node": "7.5.0",
    "babel-core": "^6.26.3"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/preset-env": "7.4.5",
    "gulp": "4.0.2",
    "gulp-babel": "8.0.0",
    "jest": "24.9.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry"
        }
      ]
    ],
    "ignore": [
      "**/excl/*"
    ]
  }
}
